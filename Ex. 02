const primeiroNome = "William";
const ultimoNome = "Costa";

// Usando operador +
const nomeCompleto1 = primeiroNome + " " + ultimoNome;

// Usando template strings
const nomeCompleto2 = `${primeiroNome} ${ultimoNome}`;

console.log(`Nome completo usando operador +: ${nomeCompleto1}`);
console.log(`Nome completo usando template strings: ${nomeCompleto2}`);
